## ============================ Configuracion del servidor ========================== ##

<> a d2r:Server;
  rdfs:label "My D2R Server";
  d2r:baseURI <http://localhost:2020/>;
  d2r:port 2020;
  d2r:vocabularyIncludeInstances true;
  
  d2r:sparqlTimeout 300;
  d2r:pageTimeout 5;
  d2r:limitPerClassMap false;
.

## ============================ Configuracion del servidor ========================== ##


## ============================ Conexion a la base de datos ========================== ##

map:database a d2rq:Database;
	d2rq:jdbcDriver "org.postgresql.Driver";
	d2rq:jdbcDSN "jdbc:postgresql://localhost/dborbis050919";
	d2rq:username "postgres";
	d2rq:password "123456";
.


## ============================ Configuración para optimización ===================== ##

 map:Configuration a d2rq:Configuration;
    d2rq:useAllOptimizations true.

## ============================ Mapeo de Investigador =============================== ##                              



##-------------------- Datos Personales -------------------##

map:Investigador a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:class vivo:FacultyMember;
	d2rq:uriPattern "individual/@@investigador.idvivo@@"; 
	d2rq:classDefinitionLabel "FacultyMember"@es; 
	.

    map:Investigador_fullName a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property rdfs:label;
	d2rq:pattern "@@investigador.nombre@@ @@investigador.apellidopat@@ @@investigador.apellidomat@@ ";
	d2rq:datatype xsd:string;
	d2rq:propertyDefinitionLabel "Nombre completo"@es;
    .
    map:Investigador_mostSpecificType a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property vitro:mostSpecificType;
	d2rq:uriPattern "http://vivoweb.org/ontology/core#FacultyMember";
	.
    
    map:Investigador_rpe a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property uaslp:idrpe;
	d2rq:column "investigador.rpe";
	d2rq:datatype xsd:string;
	d2rq:propertyDefinitionLabel "RPE"@es;
    .

	map:InvestigadorEstatus1 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property uaslp:crisEstatus1;
	d2rq:column "investigador.estatus1";
	d2rq:datatype xsd:string;
	.
	map:InvestigadorEstatus2 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property uaslp:crisEstatus2;
	d2rq:column "investigador.estatus2";
	d2rq:datatype xsd:string;
	.
	map:InvestigadorCRISID a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property uaslp:crisid;
	d2rq:column "investigador.idinvestigador";
	d2rq:datatype xsd:integer;
	d2rq:propertyDefinitionLabel "id"@es;
    .

    map:Investigador_apellidos a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property foaf:lastName;
	d2rq:datatype xsd:string;
	d2rq:pattern "@@investigador.apellidopat@@ @@investigador.apellidomat@@";
	d2rq:propertyDefinitionLabel "Apellidos"@es;

    .
    map:Investigador_givenName a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property foaf:firstName;
	d2rq:column "investigador.nombre";
	d2rq:datatype xsd:string;
	d2rq:propertyDefinitionLabel "Nombre del Investigador"@es;
    .

	map:Investigador_VCard a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property obo:ARG_2000028;
	d2rq:refersToClassMap map:VCard;
	.

	# Preferred Title
	map:VCardTitle a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:class  vcard:Title;
	d2rq:uriPattern "/vcard_titulo_@@investigador.idinvestigador@@"; 
	d2rq:classDefinitionLabel "Researcher VCard's title"@en; 
	.

	map:Investigador_preferredTitle  a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:VCardTitle;
	d2rq:property vcard:title;
	d2rq:column "investigador.preferredtitle";
	d2rq:datatype xsd:string;
	d2rq:propertyDefinitionLabel "Titulo preferido del investigador"@es;
	.


	# Enfoque Geográfico

	# Pais
	
	map:Pais a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:class vivo:Country;
	d2rq:uriPattern "pais_@@investigador.pais|urlify@@" 
	.
	map:PaisMostSpecificType a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Pais;
	d2rq:property vitro:mostSpecificType;
	d2rq:uriPattern "http://vivoweb.org/ontology/core#Country";
	.
	map:paisLabel a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Pais;
	d2rq:property rdfs:label;
	d2rq:datatype xsd:string;
	d2rq:column "investigador.pais";
	.
	map:InvestigadorPais a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property vivo:geographicFocus;
	d2rq:refersToClassMap map:Pais;
    .
    

	# Estado

	map:Estado a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:class vivo:StateOrProvince;
	d2rq:uriPattern "estado_@@investigador.estado|urlify@@" 
	.

	map:EstadoMostSpecificType a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Estado;
	d2rq:property vitro:mostSpecificType;
	d2rq:uriPattern "http://vivoweb.org/ontology/core#StateOrProvince";
	.

	map:estadoLabel a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Estado;
	d2rq:property rdfs:label;
	d2rq:datatype xsd:string;
	d2rq:column "investigador.estado";
	.

	map:InvestigadorEstado a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property vivo:geographicFocus;
	d2rq:refersToClassMap map:Estado;
	
    .


	# SCOPUS ID
	map:Investigador_scopusId a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property vivo:scopusId;
	d2rq:pattern "<a href='https://www.scopus.com/authid/detail.uri?authorId=@@scopus.scopusid@@' >@@scopus.scopusid@@</a>";
	d2rq:join "investigador.rpe <= scopus.rpe";

	.
	
	# ORCID ID

	map:ORCID a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:class owl:Thing;
	d2rq:uriPattern "https://orcid.org/@@orcid.orcidid@@"; 
	d2rq:classDefinitionLabel "ORCID ID del Investigador"@en; 
	.
	map:ORCID_mostSpecificType a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ORCID;
	d2rq:property vitro:mostSpecificType;
	d2rq:uriPattern "http://www.w3.org/2002/07/owl#Thing";
	.	

	map:Investigador_orcId a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property vivo:orcidId;
	d2rq:refersToClassMap map:ORCID;
	d2rq:join "investigador.rpe <= orcid.rpe";
	
	.
	# ---- Areas de Investigacion ---- #

	map:AreaInvestigacion a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:class owl:Thing,skos:Concept;
	d2rq:uriPattern "individual/@@areasinvestigacionfromvivo.key_area@@"; 
	d2rq:classDefinitionLabel "Area de Investigacion"@en; 
	.

	map:Area_mostSpecificType a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:AreaInvestigacion;
	d2rq:property vitro:mostSpecificType;
	d2rq:uriPattern "http://www.w3.org/2004/02/skos/core#Concept";
	.	
	map:AreaLabel a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:AreaInvestigacion;
	d2rq:property rdfs:label;
	d2rq:datatype xsd:string;
	d2rq:pattern "@@areasinvestigacionfromvivo.nombre@@ (@@areasinvestigacionfromvivo.tipo@@)"
	.	

	map:ResearchAreas a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property vivo:hasResearchArea;
	d2rq:refersToClassMap map:AreaInvestigacion;
	d2rq:join "areasporinvestigador.rpe => investigador.rpe";
	d2rq:join "areasporinvestigador.keyarea => areasinvestigacionfromvivo.key_area";
	.

	# ISI RESEARCHER ID
	map:Investigador_ISIResearcherId a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Investigador;
	d2rq:property vivo:researcherId;
	d2rq:pattern "<a href='http://www.researcherid.com/rid/@@wos.researcherid@@'>@@wos.researcherid@@</a>";
	d2rq:join "investigador.rpe <= wos.rpe";
	.

